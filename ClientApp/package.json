{
  "name": "brokkolybotfrontend",
  "version": "0.1.0",
  "private": true,
  "proxy": "https://localhost:44320",
  "type": "module",
  "dependencies": {
    "@azure/app-configuration": "^1.1.0",
    "@babel/core": "^7.13.8",
    "@types/bootstrap": "^5.0.8",
    "@types/chai": "^4.2.15",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.2",
    "@types/react-bootstrap": "^0.32.25",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.6.3",
    "bootstrap": "^4.6.0",
    "create-react-app": "^4.0.3",
    "dotenv": "^8.2.0",
    "jquery": "^3.6.0",
    "merge": "^2.1.1",
    "mocha": "^8.3.0",
    "oidc-client": "^1.11.5",
    "postcss": "^8.2.7",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.1",
    "react-cookie": "^4.0.3",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.3",
    "react-toastify": "^7.0.3",
    "reactstrap": "^8.9.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@types/react-router-dom": "^5.1.7",
    "@types/reactstrap": "^8.5.1",
    "ajv": "^6.12.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.22.0",
    "nan": "^2.14.1",
    "typescript": "^4.2.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "start": "rimraf ./buiytihld && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coveragePathIgnorePatterns": [
      "./src/*/*.types.{ts,tsx}",
      "./src/index.tsx",
      "./src/serviceWorker.ts"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 95,
        "lines": 95,
        "functions": 95
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "src/(.*)$": "<rootDir>/src/$1"
    }
  }
}
